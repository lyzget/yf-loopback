{
    "name": "product",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "id": {
            "type": "number",
            "id": true,
            "generated": true
        },
        "name": {
            "type": "string",
            "required": true
        },
        "price": {
            "type": "number"
        },
        "create_at": {
            "type": "date",
            "required": true,
            "defaultFn": "now"
        }
    },
    "validations": [],
    "relations": {
        "brand": {
            "type": "belongsTo",
            "model": "brand",
            "foreignKey": "brandId"
        }
    },
    "acls": [{
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        },
        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW",
            "property": "*"
        }
    ],
    "methods": {}
}